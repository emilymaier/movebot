<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="match_parent">
	<LinearLayout
		android:id="@+id/developerLayout1"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content">
		<TextView
			android:id="@+id/developerLatitudeLabel"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="Latitude: "/>
		<TextView
			android:id="@+id/developerLatitude"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"/>
	</LinearLayout>
	<LinearLayout
		android:id="@+id/developerLayout2"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_below="@+id/developerLayout1">
		<TextView
			android:id="@+id/developerLongitudeLabel"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="Longitude: "/>
		<TextView
			android:id="@+id/developerLongitude"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"/>
	</LinearLayout>
	<LinearLayout
		android:id="@+id/developerLayout3"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_below="@+id/developerLayout2">
		<TextView
			android:id="@+id/developerAltitudeLabel"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="Altitude: "/>
		<TextView
			android:id="@+id/developerAltitude"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"/>
	</LinearLayout>
	<LinearLayout
		android:id="@+id/developerLayout4"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_below="@+id/developerLayout3">
		<TextView
			android:id="@+id/developerSpeedLabel"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="Speed: "/>
		<TextView
			android:id="@+id/developerSpeed"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"/>
	</LinearLayout>
	<LinearLayout
		android:id="@+id/developerLayout5"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_below="@+id/developerLayout4">
		<TextView
			android:id="@+id/developerTimeLabel"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="Time: "/>
		<TextView
			android:id="@+id/developerTime"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"/>
	</LinearLayout>
</RelativeLayout>
